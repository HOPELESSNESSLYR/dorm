<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.hydropower.mapper.DormRoomHydropowerMapper">
    
    <resultMap type="DormRoomHydropower" id="DormRoomHydropowerResult">
        <result property="id"    column="ID"    />
        <result property="date"    column="date"    />
        <result property="areaNumber"    column="area_number"    />
        <result property="floorNumber"    column="floor_number"    />
        <result property="dormFloor"    column="dorm_floor"    />
        <result property="roomNumber"    column="room_number"    />
        <result property="roomHotwater"    column="room_hotwater"    />
        <result property="roomColdwater"    column="room_coldwater"    />
        <result property="roomElectric"    column="room_electric"    />
        <result property="type"    column="type"    />
    </resultMap>

    <sql id="selectDormRoomHydropowerVo">
        select ID, date, area_number, floor_number, dorm_floor, room_number, room_hotwater, room_coldwater, room_electric, type from dorm_room_hydropower
    </sql>

    <select id="selectDormRoomHydropowerList" parameterType="DormRoomHydropower" resultMap="DormRoomHydropowerResult">
        <include refid="selectDormRoomHydropowerVo"/>
        <where>  
            <if test="date != null "> and date = #{date}</if>
            <if test="areaNumber != null  and areaNumber != ''"> and area_number = #{areaNumber}</if>
            <if test="floorNumber != null  and floorNumber != ''"> and floor_number = #{floorNumber}</if>
            <if test="dormFloor != null "> and dorm_floor = #{dormFloor}</if>
            <if test="roomNumber != null  and roomNumber != ''"> and room_number = #{roomNumber}</if>
            <if test="roomHotwater != null "> and room_hotwater = #{roomHotwater}</if>
            <if test="roomColdwater != null "> and room_coldwater = #{roomColdwater}</if>
            <if test="roomElectric != null "> and room_electric = #{roomElectric}</if>
            <if test="type != null "> and type = #{type}</if>
        </where>
    </select>
    
    <select id="selectDormRoomHydropowerById" parameterType="Long" resultMap="DormRoomHydropowerResult">
        <include refid="selectDormRoomHydropowerVo"/>
        where ID = #{id}
    </select>
        
    <insert id="insertDormRoomHydropower" parameterType="DormRoomHydropower" useGeneratedKeys="true" keyProperty="id">
        insert into dorm_room_hydropower
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="date != null">date,</if>
            <if test="areaNumber != null and areaNumber != ''">area_number,</if>
            <if test="floorNumber != null and floorNumber != ''">floor_number,</if>
            <if test="dormFloor != null">dorm_floor,</if>
            <if test="roomNumber != null and roomNumber != ''">room_number,</if>
            <if test="roomHotwater != null">room_hotwater,</if>
            <if test="roomColdwater != null">room_coldwater,</if>
            <if test="roomElectric != null">room_electric,</if>
            <if test="type != null">type,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="date != null">#{date},</if>
            <if test="areaNumber != null and areaNumber != ''">#{areaNumber},</if>
            <if test="floorNumber != null and floorNumber != ''">#{floorNumber},</if>
            <if test="dormFloor != null">#{dormFloor},</if>
            <if test="roomNumber != null and roomNumber != ''">#{roomNumber},</if>
            <if test="roomHotwater != null">#{roomHotwater},</if>
            <if test="roomColdwater != null">#{roomColdwater},</if>
            <if test="roomElectric != null">#{roomElectric},</if>
            <if test="type != null">#{type},</if>
         </trim>
    </insert>

    <update id="updateDormRoomHydropower" parameterType="DormRoomHydropower">
        update dorm_room_hydropower
        <trim prefix="SET" suffixOverrides=",">
            <if test="date != null">date = #{date},</if>
            <if test="areaNumber != null and areaNumber != ''">area_number = #{areaNumber},</if>
            <if test="floorNumber != null and floorNumber != ''">floor_number = #{floorNumber},</if>
            <if test="dormFloor != null">dorm_floor = #{dormFloor},</if>
            <if test="roomNumber != null and roomNumber != ''">room_number = #{roomNumber},</if>
            <if test="roomHotwater != null">room_hotwater = #{roomHotwater},</if>
            <if test="roomColdwater != null">room_coldwater = #{roomColdwater},</if>
            <if test="roomElectric != null">room_electric = #{roomElectric},</if>
            <if test="type != null">type = #{type},</if>
        </trim>
        where ID = #{id}
    </update>

    <delete id="deleteDormRoomHydropowerById" parameterType="Long">
        delete from dorm_room_hydropower where ID = #{id}
    </delete>

    <delete id="deleteDormRoomHydropowerByIds" parameterType="String">
        delete from dorm_room_hydropower where ID in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>